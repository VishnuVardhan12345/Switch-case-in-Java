    public static void main(String[] args) {
        int choice;
        System.out.println("Pick One:\n1.Hello\n2.Hi\n3.Bye\n");
        System.out.println("Enter a value");
        Scanner s =new Scanner(System.in);
        choice = s.nextInt();
        switch(choice)
        {
            case 1 :System.out.println("You said Hello");
            break;
            case 2 :System.out.println("You said Hi");
            break;
            case 3 :System.out.println("You said Bye");
        }
                
    }
